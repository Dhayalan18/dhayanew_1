n=[1,2,3,4,0,7,8,9,5,6]
n.sort()
print("The sorted array is"n)                                                       #sorted array
length=len(n)-1
maxi=n[length]
print("The maximum element is",maxi)                                                #maximum element
mini=n[0]
print("The minimum element is",mini)                                                #minimum element

