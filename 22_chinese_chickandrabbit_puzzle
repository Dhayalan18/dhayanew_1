def solve(heads,legs):
    error_msg="No solution"
    if(heads<legs):
        chicken_count=0
        rabbit_count=0
        rabbit_count=(legs-(2*heads))//2
        chicken_count=heads-rabbit_count
        a=(2*chicken_count)+(4*rabbit_count)
        if(a==legs):
            print(chicken_count,rabbit_count)
        else:
            print(error_msg)
    else:
        print(error_msg)
solve(38,131)

